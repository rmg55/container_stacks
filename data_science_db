FROM rowangaffney/data_science_im_rs

USER $NB_UID

LABEL maintainer="Rowan Gaffney <rowan.gaffney@ars.usda.gov>"

RUN conda install -c conda-forge postgis

RUN fix-permissions /home/$NB_USER
RUN fix-permissions $CONDA_DIR

RUN conda init

